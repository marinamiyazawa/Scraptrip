<div class="container">
	<div class="row">
		<div class="col-xs-8 col-xs-offset-2">
			<h2>User index</h2>
			<% @users.each do |user| %>
				<table class="table">
					<tr>
						<th><%= attachment_image_tag(user, :image, size:'60x60') %></th>
						<th><%= user.nick_name %></th>
						<th><%= user.email %></th>
					</tr>
				</table>
			<% end %>
		</div>
	</div>
</div>